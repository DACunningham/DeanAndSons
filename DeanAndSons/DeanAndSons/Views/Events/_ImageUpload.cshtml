@using DeanAndSons.Models.WAP.ViewModels
@model EventCreateViewModel

<div class="form-group">
    @Html.LabelFor(model => model.Images, "Body Image", htmlAttributes: new { @class = "control-label col-md-2" })
    <div class="col-md-10">
        @Html.TextBoxFor(model => model.Images, new { type = "file" })
        @Html.ValidationMessageFor(model => model.Images, "", new { @class = "text-danger" })
    </div>
</div>
